<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="fdm_report_saleorder_document_inherit" inherit_id="sale.report_saleorder_document">
        <xpath expr="//td[@name='td_name']" position="replace">
            <td name="td_name">
                <t t-set="description_name" t-value="line.name.replace('[%s]' %line.product_id.default_code,'')"/>
                <span t-esc="description_name"/>
            </td>

        </xpath>
    </template>
</odoo>